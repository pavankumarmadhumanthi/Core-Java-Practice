package com.example;

import java.util.Scanner;

public class CheckingPrimeNumber {
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int num = sc.nextInt();
		int count=0;
		for(int i=2;i<num/2;i++) {
			if(num%i==0) {
				System.out.println("given number is not a prime number");
			}
			
		}
		
	}

}
