package com.example;

public class CheckingVowelInString {
	public static void main(String[] args) {
		String str="pavan";
		String vowel="aeiou";
		for(int i=0;i<str.length();i++) {
			char value=str.charAt(i);
			for(int j=0;j<vowel.length();j++) {
				if(vowel.charAt(j)==value) {
					System.out.println("vowel found");
					break;
				}
			}
		
		}
	}

}
